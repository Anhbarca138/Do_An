<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_nam_form" model="ir.ui.view">
            <field name="name">nam</field>
            <field name="model">nam</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <div class="oe_read_only" style="font-size:25px"></div>
                        <group col="4" string="Thông tin chung">
                            <field name="id" />

                            <field name="nam" />
                            <field name="mo_ta" />
                            <!-- <field name="van_ban_den_ids" /> -->
                            <!-- <field name="van_ban_di_ids" /> -->

                        </group>
                    </sheet>

                </form>
            </field>
        </record>

        <record id="view_nam_tree" model="ir.ui.view">
            <field name="name">nam</field>
            <field name="model">nam</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="id" />

                    <field name="nam" />
                    <field name="mo_ta" />
                    <field name="van_ban_di_ids" />
                    <field name="van_ban_den_ids" />


                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="nam_search">
            <field name="model">nam</field>
            <field name="arch" type="xml">
                <search>
                    <field name="id" />

                    <field name="nam" />
                    <field name="mo_ta" />
                </search>
            </field>
        </record>
        <record id="action_nam" model="ir.actions.act_window">
            <field name="name">Năm</field>
            <field name="res_model">nam</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="nam_search" />
        </record>
    </data>
</odoo>
        
    